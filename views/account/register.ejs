<section class="portfolio-block block-intro" style="background: white;">
    <div class=container>
        <div class="data-form">
            <form class="form-signin" action="/account/create" method="POST">
              <h2 class="form-signin-heading">Join the Amano Family</h2>
              <input type="text" name="first" id="inputFirstName" class="form-control" placeholder="First Name" autofocus>
              <input type="text" name="last" id="inputLastName" class="form-control" placeholder="Last Name">
              <input type="password" name="password" id="inputPassword" class="form-control" placeholder="New Password" required>
              <input type="email" name="email" id="inputEmail" class="form-control" placeholder="Email Address" required>
              <input type="text" name="zipCode" id="inputZipCode" class="form-control" placeholder="Zip Code" required>
              <br />
              <button class="btn btn-lg btn-primary btn-block" style="background-color:#23233d; border-color:#d6ecea;" type="submit">Register New Account</button>
            </form>
          </div>
    </div>
</section>

